<Page x:Class="LoginApp.Page0"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LoginApp"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="800"
      Title="Page0">

    <Grid>
        <Image Source="/image/INC.png" Stretch="UniformToFill" Margin="0,10,0,0" Style="{StaticResource imgClose }" Height="262" Width="368" HorizontalAlignment="Center"/>
        <Button Content="GO" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,410,0,0" Click="Gobutton" Height="54" Width="204" Style="{StaticResource mainbutton}"/>
    </Grid>
</Page>
